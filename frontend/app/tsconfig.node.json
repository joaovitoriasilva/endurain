{
  "extends": "@vue/tsconfig/tsconfig.json",
  "include": [
    "vite.config.*",
    "vitest.config.*",
    ".eslintrc.cjs"
  ],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "allowJs": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "types": ["node"]
  }
}
